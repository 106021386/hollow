import java.util.Scanner;

public class Main {

    public static void main(String[] args) {
        Scanner scm =new Scanner(System.in);
        int a=scm.nextInt();
        int b=scm.nextInt();
        System.out.println(a+b);
        System.out.println(a-b);
        System.out.println(a*b);
        System.out.println(a/b);


    }
}
